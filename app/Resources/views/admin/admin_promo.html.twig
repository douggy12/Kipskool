{% extends 'base.html.twig' %}
{% block body %}
<h1>{{ ecole.nom }}</h1>
    <table>
        <td><a href="{{ path('admin_ecole', {'ecole_id': ecole.id}) }}">Ecole</a></td>
        <td><a href="{{ path('admin_promo', {'ecole_id': ecole.id}) }}">Promo</a></td>
        <td><a href="">Perso</a></td>
        <td><a href="{{ path('ecole_show', {'id': ecole.id}) }}">back to {{ ecole.nom }} view</a></td>
    </table>
    <form action="" method="post">
    <select id="promoSelect" onchange="this.form.submit()">
        {% for promo in ecole.promos %}
            <option value="{{ promo. }}" onselect="">{{ promo.nom }}</option>
        {% endfor %}
    </select>
    </form>
{% endblock %}
